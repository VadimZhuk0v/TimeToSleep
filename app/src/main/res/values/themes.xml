<resources>

    <style name="Theme.TimeToSleep" parent="Theme.Material3.Dark.NoActionBar">
        <item name="android:windowBackground">@color/black</item>
        <item name="android:windowTranslucentNavigation">true</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>

    <style name="Theme.TimeToSleep.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.TimeToSleep.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.TimeToSleep.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>